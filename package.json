{
  "name": "pontual",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "npm run build:ssr",
    "serve:local-net": "ng serve --host 0.0.0.0",
    "serve:local-net-ssl": "ng serve --host 0.0.0.0 --ssl",
    "build:github": "ng build --output-path docs --base-href https://isaquiasmarques.github.io/pontual",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run pontual:serve-ssr",
    "serve:ssr": "node dist/pontual/server/main.js",
    "build:ssr": "ng build && ng run pontual:server",
    "prerender": "ng run pontual:prerender",
    "vercel-build": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "express": "^4.18.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.8",
    "@angular/cli": "~16.0.4",
    "@angular/compiler-cli": "^16.0.8",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.5",
    "autoprefixer": "^10.4.16",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.1.0"
  }
}
